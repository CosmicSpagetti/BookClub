<h1>Add new Book</h1>
<%= form_for @books do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <br>
  <%= f.label :pages %>
  <%= f.text_field :pages %><br>
  <br>
  <%= f.label :published %>
  <%= f.text_field :published %><br>
  <br>
  <%= f.label :image %>
  <%= f.text_field :image %><br>
  <br>

  <%= f.submit 'Create Book' %>
<% end %>
